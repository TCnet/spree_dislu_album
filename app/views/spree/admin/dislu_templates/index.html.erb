<% content_for :page_title do %>
   <%= plural_resource_name(Spree::DisluTemplate) %>
<% end %>

  

<%= form_for([:admin, @dislu_template], html: { multipart: true }) do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    
  <div data-hook="buttons" class="form-actions ">
    <%= button Spree.t('actions.create'), 'ok', 'submit', {class: 'btn-success'} %>
  </div>
  </fieldset>
<% end %>

<% if can?(:create, Spree::DisluTemplate) %>

<table class="table" id="listing_dislu_templates">
    <thead>
      <tr data-hook="admin_dislu_templates_index_headers">
        <th><%= Spree.t(:date) %></th>
        <th><%= Spree.t(:name) %></th>
        <th data-hook="admin_dislu_templates_index_header_actions" class="actions"></th>
      </tr>
    </thead>
    <tbody>
<% @templates.each do |f|%>
	<tr>
		
		<td><%= f.created_at%></td>
    <td><%= f.name%></td>
		 <td class="actions actions-1 text-right" data-hook="admin_dislu_templates_index_row_actions">
        <%= link_to_edit f, no_text: true if can?(:edit, f)  %>
        <%= link_to_delete f, no_text: true if can?(:delete, f)  %>
            </td>
	</tr>
	<% end %>
    </tbody>
    </table>



 <%end%>



